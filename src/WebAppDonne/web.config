<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="." inheritInChildApplications="false">
		<system.webServer>
			<security>
				<requestFiltering>
					<verbs allowUnlisted="false">
						<add verb="GET" allowed="true" />
						<add verb="POST" allowed="true" />
						<add verb="DELETE" allowed="true" />
						<add verb="PUT" allowed="true" />
						<add verb="OPTIONS" allowed="true" />
						<add verb="HEAD" allowed="true" />
						<add verb="PROFIND" allowed="true" />
						<add verb="PROPPATCH" allowed="true" />
						<add verb="MKCOL" allowed="true" />
						<add verb="COPY" allowed="true" />
						<add verb="MOVE" allowed="true" />
						<add verb="LOCK" allowed="true" />
					</verbs>
				</requestFiltering>
			</security>
			<modules>
				<remove name="WebDAVModule" />
			</modules>
			<handlers>
				<remove name="WebDAV" />
				<add name="aspNetCore" path="*" verb="PUT,GET,HEAD,POST,DELETE,OPTIONS,PROPFIND,PROPPATCH,MKCOL,COPY,MOVE,LOCK" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
			</handlers>
			<aspNetCore processPath="C:\inetpub\wwwroot\WebAppDonne.exe" 
						stdoutLogEnabled="true" 
						stdoutLogFile="\\?\%home%\LogFiles\stdout"  />
		</system.webServer>
	</location>
</configuration>